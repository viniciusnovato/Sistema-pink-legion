{
  "version": 2,
  "functions": {
    "api/generate-pdf.py": {
      "runtime": "python@3.12"
    },
    "api/generate_confissao_pdf.py": {
      "runtime": "python@3.12"
    }
  },
  "routes": [
    {
      "src": "/api/generate-pdf",
      "dest": "/api/generate-pdf.py"
    },
    {
      "src": "/api/generate_confissao_pdf",
      "dest": "/api/generate_confissao_pdf.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}